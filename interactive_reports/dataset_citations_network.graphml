<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
         http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    
    <key id="type" for="node" attr.name="type" attr.type="string"/>
    <key id="confidence" for="node" attr.name="confidence" attr.type="double"/>
    <key id="cited_by" for="node" attr.name="cited_by" attr.type="int"/>
    <key id="weight" for="edge" attr.name="weight" attr.type="double"/>
    
    <graph id="dataset_citations" edgedefault="undirected">
        <node id="dataset_001">
            <data key="type">dataset</data>
            <data key="confidence">1.0</data>
            <data key="cited_by">15</data>
        </node>
        <node id="citation_001">
            <data key="type">citation</data>
            <data key="confidence">0.8</data>
            <data key="cited_by">25</data>
        </node>
        <node id="bridge_001">
            <data key="type">bridge</data>
            <data key="confidence">0.9</data>
            <data key="cited_by">40</data>
        </node>
        <edge source="dataset_001" target="citation_001">
            <data key="weight">0.8</data>
        </edge>
        <edge source="citation_001" target="bridge_001">
            <data key="weight">0.9</data>
        </edge>
    </graph>
</graphml>